<h2>users</h2>
<table cellspacing='0px'>
	<thead>
	<tr>
		<% for(var attr in Mongee.Models.User.attributes){%>
			<% if(attr == 'id') continue;%>
			<th><%= attr%> </th>	 
		<%}%>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= $.View('//mongee/views/user/list',{users: users})%>
	</tbody>
</table>
<h2>New user</h2>
<form>
	<% for(var attr in Mongee.Models.User.attributes){ %>
		<% if(attr == 'id') continue;%>
		<div>
			<label><%= attr %> </label><br />
			<input type="text" value="" name="<%= attr %>"/>
		</div>
	<%}%>
	<input type='submit' value='Create'/>
</form>